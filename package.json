{
  "name": "fhir-server-ui",
  "version": "1.0.0",
  "description": "UI to display FHIR resources in a web browser",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc && yarn copy-files",
    "build-clean": "yarn clean && tsc && yarn copy-files",
    "clean": "rm -rf ./dist",
    "start": "node dist/index.js",
    "dev": "nodemon",
    "copy-files": "cp ./src/oauth/*.html ./dist/oauth/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.4",
    "cookie-parser": "^1.4.6",
    "crypto": "^1.0.1",
    "express": "^4.18.2",
    "express-http-context": "^1.2.4",
    "express-useragent": "^1.0.15",
    "helmet": "^7.0.0",
    "jwks-rsa": "^3.0.1",
    "openid-client": "^5.5.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "superagent": "^8.1.2",
    "var": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.19",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@types/async": "^3.2.20",
    "@types/cookie-parser": "^1.4.4",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/fhir": "^0.0.37",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "@types/passport-jwt": "^3.0.9",
    "@types/superagent": "^4.1.18",
    "@types/supertest": "^2.0.12",
    "jest": "^29.7.0",
    "jest-matcher-utils": "^29.7.0",
    "jose": "^4.14.6",
    "nock": "^13.3.3",
    "nodemon": "^3.0.1",
    "npm-check-updates": "^16.14.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "jest": {
    "watchman": false,
    "watchPathIgnorePatterns": [
      "globalConfig"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/web"
    ],
    "setupFiles": [
      "<rootDir>/jest/setEnvVars.js"
    ]
  },
  "tsconfig.json": "tsconfig.json"
}
